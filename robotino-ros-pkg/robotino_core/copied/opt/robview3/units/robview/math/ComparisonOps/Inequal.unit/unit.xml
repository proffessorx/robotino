<?xml version="1.0" encoding="UTF-8"?>
<unit:unit
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://easykit.informatik.tu-muenchen.de/schema/unit unit.xsd"
	xmlns:plugin="http://easykit.informatik.tu-muenchen.de/schema/plugin"
	xmlns:unit="http://easykit.informatik.tu-muenchen.de/schema/unit"
	xmlns:var="http://easykit.informatik.tu-muenchen.de/schema/variable"
	xmlns:tmpl="http://easykit.informatik.tu-muenchen.de/schema/template"
	xmlns:tr="http://easykit.informatik.tu-muenchen.de/schema/translation">

	<unit:translations>
	</unit:translations>

	<unit:unittypes>
		<unit:unittype unit:id="REC GmbH Basic Inequality">
			<unit:typeTemplates />
			<unit:connectors>
				<unit:in>
					<unit:connector var:type="float" var:varname="op1">
						<var:default var:val="0"/>
					</unit:connector>
					<unit:connector var:type="float" var:varname="op2">
						<var:default var:val="0"/>
					</unit:connector>
				</unit:in>
				<unit:out>
					<unit:connector var:type="bool" var:varname="ineq">
						<var:default var:val="0"/>
					</unit:connector>
				</unit:out>
			</unit:connectors>
			<unit:state/>

			<unit:guiinfo>
				<unit:connectors>
					<unit:in>
						<unit:layout unit:varname="op1" unit:side="left"/>
						<unit:layout unit:varname="op2" unit:side="left"/>
					</unit:in>
					<unit:out>
						<unit:layout unit:varname="ineq" unit:side="right"/>
					</unit:out>
				</unit:connectors>
				<unit:state/>

				<unit:description>
					<unit:name>Inequality</unit:name>
					<unit:icon unit:filename="inequal.png"/>
					<unit:connectors>
						<unit:in>
							<unit:info unit:varname="op1">
								<unit:description>Input[ 1]</unit:description>
							</unit:info>
							<unit:info unit:varname="op2">
								<unit:description>Input[ 2]</unit:description>
							</unit:info>
						</unit:in>
						<unit:out>
							<unit:info unit:varname="ineq">
								<unit:description>Output</unit:description>
							</unit:info>
						</unit:out>
					</unit:connectors>
					<unit:state/>
				</unit:description>
			</unit:guiinfo>

			<unit:plugins>
				<unit:plugin plugin:type="simulation" plugin:library="unit_robview_math_inequal_simulation"/>
			</unit:plugins>

		</unit:unittype>
	</unit:unittypes>
</unit:unit>
