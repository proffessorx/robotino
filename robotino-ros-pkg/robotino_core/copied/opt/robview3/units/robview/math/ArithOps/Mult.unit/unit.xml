<?xml version="1.0" encoding="UTF-8"?>
<unit:unit
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://easykit.informatik.tu-muenchen.de/schema/unit unit.xsd"
	xmlns:plugin="http://easykit.informatik.tu-muenchen.de/schema/plugin"
	xmlns:unit="http://easykit.informatik.tu-muenchen.de/schema/unit"
	xmlns:var="http://easykit.informatik.tu-muenchen.de/schema/variable"
	xmlns:tmpl="http://easykit.informatik.tu-muenchen.de/schema/template"
	xmlns:tr="http://easykit.informatik.tu-muenchen.de/schema/translation">

	<unit:translations>
	</unit:translations>

	<unit:unittypes>
		<unit:unittype unit:id="REC GmbH Basic Multiplication">
			<unit:typeTemplates/>
			<unit:connectors>
				<unit:in>
					<unit:connector var:type="float" var:varname="f1">
						<var:default var:val="1"/>
					</unit:connector>
					<unit:connector var:type="float" var:varname="f2">
						<var:default var:val="1"/>
					</unit:connector>
					<unit:connector var:type="float" var:varname="f3">
						<var:default var:val="1"/>
					</unit:connector>
					<unit:connector var:type="float" var:varname="f4">
						<var:default var:val="1"/>
					</unit:connector>
					<unit:connector var:type="float" var:varname="f5">
						<var:default var:val="1"/>
					</unit:connector>
					<unit:connector var:type="float" var:varname="f6">
						<var:default var:val="1"/>
					</unit:connector>
					<unit:connector var:type="float" var:varname="f7">
						<var:default var:val="1"/>
					</unit:connector>
					<unit:connector var:type="float" var:varname="f8">
						<var:default var:val="1"/>
					</unit:connector>
					<unit:connector var:type="float" var:varname="f9">
						<var:default var:val="1"/>
					</unit:connector>
					<unit:connector var:type="float" var:varname="f10">
						<var:default var:val="1"/>
					</unit:connector>
				</unit:in>
				<unit:out>
					<unit:connector var:type="float" var:varname="product">
						<var:default var:val="0"/>
					</unit:connector>
				</unit:out>
			</unit:connectors>
			<unit:state/>

			<unit:guiinfo>
				<unit:connectors>
					<unit:in>
						<unit:layout unit:varname="f1" unit:side="left"/>
						<unit:layout unit:varname="f2" unit:side="left"/>
						<unit:layout unit:varname="f3" unit:side="left" unit:display="hidden"/>
						<unit:layout unit:varname="f4" unit:side="left" unit:display="hidden"/>
						<unit:layout unit:varname="f5" unit:side="left" unit:display="hidden"/>
						<unit:layout unit:varname="f6" unit:side="left" unit:display="hidden"/>
						<unit:layout unit:varname="f7" unit:side="left" unit:display="hidden"/>
						<unit:layout unit:varname="f8" unit:side="left" unit:display="hidden"/>
						<unit:layout unit:varname="f9" unit:side="left" unit:display="hidden"/>
						<unit:layout unit:varname="f10" unit:side="left" unit:display="hidden"/>
					</unit:in>
					<unit:out>
						<unit:layout unit:varname="product" unit:side="right"/>
					</unit:out>
				</unit:connectors>
				<unit:state/>

				<unit:description>
					<unit:name>Multiplication</unit:name>
					<unit:icon unit:filename="mult.png"/>
					<unit:connectors>
						<unit:in>
							<unit:info unit:varname="f1">
								<unit:description>Factor[ 1]</unit:description>
							</unit:info>
							<unit:info unit:varname="f2">
								<unit:description>Factor[ 2]</unit:description>
							</unit:info>
							<unit:info unit:varname="f3">
								<unit:description>Factor[ 3]</unit:description>
							</unit:info>
							<unit:info unit:varname="f4">
								<unit:description>Factor[ 4]</unit:description>
							</unit:info>
							<unit:info unit:varname="f5">
								<unit:description>Factor[ 5]</unit:description>
							</unit:info>
							<unit:info unit:varname="f6">
								<unit:description>Factor[ 6]</unit:description>
							</unit:info>
							<unit:info unit:varname="f7">
								<unit:description>Factor[ 7]</unit:description>
							</unit:info>
							<unit:info unit:varname="f8">
								<unit:description>Factor[ 8]</unit:description>
							</unit:info>
							<unit:info unit:varname="f9">
								<unit:description>Factor[ 9]</unit:description>
							</unit:info>
							<unit:info unit:varname="f10">
								<unit:description>Factor[ 10]</unit:description>
							</unit:info>
						</unit:in>
						<unit:out>
							<unit:info unit:varname="product">
								<unit:description>Product</unit:description>
							</unit:info>
						</unit:out>
					</unit:connectors>
					<unit:state/>
				</unit:description>
			</unit:guiinfo>

			<unit:plugins>
				<unit:plugin plugin:type="simulation" plugin:library="unit_robview_math_mult_simulation"/>
				<unit:plugin plugin:type="dialog" plugin:library="unit_robview_math_mult_gui"/>
			</unit:plugins>

		</unit:unittype>
	</unit:unittypes>
</unit:unit>
