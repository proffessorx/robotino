<?xml version="1.0" encoding="UTF-8"?>
<unit:unit
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://easykit.informatik.tu-muenchen.de/schema/unit unit.xsd"
	xmlns:plugin="http://easykit.informatik.tu-muenchen.de/schema/plugin"
	xmlns:unit="http://easykit.informatik.tu-muenchen.de/schema/unit"
	xmlns:var="http://easykit.informatik.tu-muenchen.de/schema/variable"
	xmlns:tmpl="http://easykit.informatik.tu-muenchen.de/schema/template"
	xmlns:tr="http://easykit.informatik.tu-muenchen.de/schema/translation">

	<unit:translations>
	</unit:translations>
	
	<unit:unittypes>
		<unit:unittype unit:id="REC GmbH Basic Subtraction">
			<unit:typeTemplates/>
			<unit:connectors>
				<unit:in>
					<unit:connector var:type="float" var:varname="minuend">
						<var:default var:val="0" />
					</unit:connector>
					<unit:connector var:type="float" var:varname="1">
						<var:default var:val="0"/>
					</unit:connector>
					<unit:connector var:type="float" var:varname="2">
						<var:default var:val="0"/>
					</unit:connector>
					<unit:connector var:type="float" var:varname="3">
						<var:default var:val="0"/>
					</unit:connector>
					<unit:connector var:type="float" var:varname="4">
						<var:default var:val="0"/>
					</unit:connector>
					<unit:connector var:type="float" var:varname="5">
						<var:default var:val="0"/>
					</unit:connector>
					<unit:connector var:type="float" var:varname="6">
						<var:default var:val="0"/>
					</unit:connector>
					<unit:connector var:type="float" var:varname="7">
						<var:default var:val="0"/>
